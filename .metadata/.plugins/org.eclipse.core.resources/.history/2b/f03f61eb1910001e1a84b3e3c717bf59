package com.citi.demo.assign;

public class SeriesPrintingAssignment {

	public static void main(String[] args) {
	 
		for (int i = 0;i<6;i++) {
			for (int j = 1; j<i;j++) {
			//	System.out.print(i);
				System.out.print(j);
			}
		    System.out.println();
		}
		
		for (int i = 6;i>1;i--) {
			for (int j = 5; j>1;j--) {
				System.out.print(i);
				
			}
		    System.out.println();
		}

	}

}
